{
  "displayName": "Progress Bar",
  "defaultWidth": 240,
  "defaultHeight": 24,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "color_unfinished",
      "displayName": "Unfinished-Color",
      "type": "color",
      "default": "#99D8C9"
    },
    {
      "name": "color_finished",
      "displayName": "Finished-Color",
      "type": "color",
      "default": "#2CA25F"
    },
    {
      "name": "percentage",
      "displayName": "Percentage",
      "type": "number",
      "default": 0
    },
    {
      "name": "isNumberShown",
      "displayName": "Show Percentage",
      "type": "boolean",
      "default": true
    }
  ]
}
