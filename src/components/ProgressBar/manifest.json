{
  "displayName": "Progress Bar",
  "defaultWidth": 240,
  "defaultHeight": 24,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "color_unfinished",
      "displayName": "Unfinished-Color",
      "type": "color",
      "default": "#99D8C9"
    },
    {
      "name": "color_finished",
      "displayName": "Finished-Color",
      "type": "color",
      "default": "#2CA25F"
    },
    {
      "name": "value",
      "displayName": "Current Value",
      "type": "number",
      "default": 0
    },
    {
      "name": "maxValue",
      "displayName": "Maximum Value",
      "type": "number",
      "default": 100
    },
    {
      "name": "isNumberShown",
      "displayName": "Show Percentage",
      "type": "boolean",
      "default": true
    },
    {
      "name": "borderWidth",
      "displayName": "Border Width",
      "type": "number",
      "default": 2
    },
    {
      "name": "borderColor",
      "displayName": "Border Color",
      "type": "color",
      "default": "#FFD700"
    },
    {
      "name": "borderRadius",
      "displayName": "Border Radius",
      "type": "number",
      "default": 2
    }
  ]
}
